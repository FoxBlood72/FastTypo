<br>
<%
for(var i = contacts.length - 1; i >= 0; i--)
{
    var date = new Date(contacts[i]["time"])
    var format = date.getHours() + ':' + date.getMinutes() + ' ' + date.getDate() + '.' + date.getMonth() + '.' + date.getFullYear();
    %>
    <div class="admin-content">
        <div>
            <h1 style="color:white;">Email: <%= contacts[i]["email"] %></h1>
            <p>Sent on <%= format %></p>
            <p>Message: <%= contacts[i]["content"] %> </p>
        </div>
    </div>
    <%
}
%>
